@article{ WOS:000401735900002,
Author = {Slotnick, Scott D.},
Title = {Resting-state fMRI data reflects default network activity rather than
   null data: A defense of commonly employed methods to correct for
   multiple comparisons},
Journal = {COGNITIVE NEUROSCIENCE},
Year = {2017},
Volume = {8},
Number = {3},
Pages = {141-143},
Abstract = {Analysis of functional magnetic resonance imaging (fMRI) data typically
   involves over one hundred thousand independent statistical tests;
   therefore, it is necessary to correct for multiple comparisons to
   control familywise error. In a recent paper, Eklund, Nichols, and
   Knutsson used resting-state fMRI data to evaluate commonly employed
   methods to correct for multiple comparisons and reported unacceptable
   rates of familywise error. Eklund et al.'s analysis was based on the
   assumption that resting-state fMRI data reflect null data; however,
   their null data' actually reflected default network activity that
   inflated familywise error. As such, Eklund et al.'s results provide no
   basis to question the validity of the thousands of published fMRI
   studies that have corrected for multiple comparisons or the commonly
   employed methods to correct for multiple comparisons.},
