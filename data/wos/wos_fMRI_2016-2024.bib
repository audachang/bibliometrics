@article{ WOS:000668680200001,
Author = {Teipel, Stefan J. and Brueggen, Katharina and Temp, Anna Gesine Marie
   and Jakobi, Kristina and Weber, Marc-Andre and Berger, Christoph},
Title = {Simultaneous Assessment of Electroencephalography Microstates and
   Resting State Intrinsic Networks in Alzheimer's Disease and Healthy
   Aging},
Journal = {FRONTIERS IN NEUROLOGY},
Year = {2021},
Volume = {12},
Month = {JUN 17},
Abstract = {Electroencephalography (EEG) microstate topologies may serve as building
   blocks of functional brain activity in humans. Here, we studied the
   spatial and temporal correspondences between simultaneously acquired EEG
   microstate topologies and resting state functional MRI (rs-fMRI)
   intrinsic networks in 14 patients with Alzheimer's disease (AD) and 14
   healthy age and sex matched controls. We found an anteriorisation of EEG
   microstates' topologies in AD patients compared with controls; this
   corresponded with reduced spatial expression of default mode and
   increased expression of frontal lobe networks in rs-fMRI. In a
   hierarchical cluster analysis the time courses of the EEG microstates
   were associated with the time courses of spatially corresponding rs-fMRI
   networks. We found prevalent negative correlations of time courses
   between anterior microstate topologies and posterior rs-fMRI components
   as well as between posterior microstate topology and anterior rs-fMRI
   components. These negative correlations were significantly more
   expressed in controls than in AD patients. In conclusion, our data
   support the notion that the time courses of EEG microstates underlie the
   temporal expression of rs-fMRI networks. Furthermore, our findings
