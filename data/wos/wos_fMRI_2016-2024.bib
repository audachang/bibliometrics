Title = {Reliability of neural food cue-reactivity in participants with obesity
   undergoing bariatric surgery: a 26-week longitudinal fMRI study},
Journal = {EUROPEAN ARCHIVES OF PSYCHIATRY AND CLINICAL NEUROSCIENCE},
Year = {2021},
