@article{ WOS:000442437400010,
Author = {Liu, Ying and Yao, Li and Zhao, Xiaojie},
Title = {Estimating Memory Load by a Combination of Electroencephalography and
   Functional Magnetic Resonance Imaging Using Different Construction of
   Sample Set},
Journal = {JOURNAL OF MEDICAL IMAGING AND HEALTH INFORMATICS},
Year = {2018},
Volume = {8},
Number = {7},
Pages = {1391-1396},
Month = {SEP},
Abstract = {Previous studies have indicated that both electroencephalography (EEG)
   and functional magnetic resonance imaging (fMRI) features can be used to
   estimate memory load. The influence of sample set construction on the
   estimation performance and the benefit of combining EEG and fMRI
   features remain unclear. In this study, we extracted spectral power and
   event-related-potentials from the Pz electrode and 9 electrodes in
   related lobes as the EEG feature and extracted the beta values of the
   activation brain regions as the fMRI feature at the two sample set
   construction levels of trial and run. Then we combined EEG and fMRI
   features. The results showed that the classification performance of
   memory load at the run level got higher classification accuracy when
   using rapid event-related experimental design. As for EEG features, the
   classification performance of memory load was related to channel
   selection. The combination of EEG and fMRI features did not contribute
   to load classification accuracy.},
