@article{ WOS:000537517800007,
Author = {Komaki, Yuji and Debacker, Clement and Djemai, Boucif and Ciobanu, Luisa
   and Tsurugizawa, Tomokazu and Le Bihan, Denis},
Title = {Differential effects of aquaporin-4 channel inhibition on BOLD fMRI and
   diffusion fMRI responses in mouse visual cortex},
Journal = {PLOS ONE},
Year = {2020},
Volume = {15},
Number = {5},
Month = {MAY 21},
Abstract = {The contribution of astrocytes to the BOLD fMRI and DfMRI responses in
   visual cortex of mice following visual stimulation was investigated
   using TGN-020, an aquaporin 4 (AQP4) channel blocker, acting as an
   astrocyte function perturbator. Under TGN-020 injection the amplitude of
   the BOLD fMRI response became significantly higher. In contrast no
   significant changes in the DfMRI responses and the electrophysiological
   responses were observed. Those results further confirm the implications
   of astrocytes in the neurovascular coupling mechanism underlying BOLD
   fMRI, but not in the DfMRI responses which remained unsensitive to
   astrocyte function perturbation.},
