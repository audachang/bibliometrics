@article{ WOS:000398100400026,
Author = {Sun, H. and Seres, P. and Wilman, A. H.},
Title = {Structural and functional quantitative susceptibility mapping from
   standard fMRI studies},
Journal = {NMR IN BIOMEDICINE},
Year = {2017},
