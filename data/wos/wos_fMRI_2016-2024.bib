@article{ WOS:000403773300005,
Author = {Takamura, T. and Hanakawa, T.},
Title = {Clinical utility of resting-state functional connectivity magnetic
   resonance imaging for mood and cognitive disorders},
Journal = {JOURNAL OF NEURAL TRANSMISSION},
