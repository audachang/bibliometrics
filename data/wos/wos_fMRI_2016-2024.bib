@article{ WOS:000858043200001,
Author = {Klugah-Brown, Benjamin and Yu, Yue and Hu, Peng and Agoalikum, Elijah
   and Liu, Congcong and Liu, Xiqin and Yang, Xi and Zeng, Yixu and Zhou,
   Xinqi and Yu, Xin and Rypma, Bart and Michael, Andrew M. and Li, Xiaobo
   and Becker, Benjamin and Biswal, Bharat},
Title = {Effect of surgical mask on fMRI signals during task and rest},
Journal = {COMMUNICATIONS BIOLOGY},
Year = {2022},
Volume = {5},
Number = {1},
Month = {SEP 21},
Abstract = {Wearing a face mask has become essential to contain the spread of
   COVID-19 and has become mandatory when collecting fMRI data at most
   research institutions. Here, we investigate the effects of wearing a
   surgical mask on fMRI data in n = 37 healthy participants. Activations
   during finger tapping, emotional face matching, working memory tasks,
   and rest were examined. Preliminary fMRI analyses show that despite the
   different mask states, resting-state signals and task activations were
   relatively similar. Resting-state functional connectivity showed
   negligible attenuation patterns in mask-on compared with mask-off.
   Task-based ROI analysis also demonstrated no significant difference
   between the two mask states under each contrast investigated.
   Notwithstanding the overall insignificant effects, these results
   indicate that wearing a face mask during fMRI has little to no
   significant effect on resting-state and task activations.
   Wearing a face mask during fMRI has little to no significant effect on
   resting-state and task activations.},
