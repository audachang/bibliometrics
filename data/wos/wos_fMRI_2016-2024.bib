@article{ WOS:000701021200002,
Author = {Zerweck, Leonie and Roder, Constantin and Hauser, Till-Karsten and
   Thurow, Johannes and Mengel, Annerose and Tatagiba, Marcos and Khan,
   Nadia and Meyer, Philipp T. and Ernemann, Ulrike and Klose, Uwe},
Title = {Hemodynamic evaluation of patients with Moyamoya Angiopathy: comparison
   of resting-state fMRI to breath-hold fMRI and {[}<SUP>15</SUP>O]water
   PET},
Journal = {NEURORADIOLOGY},
Year = {2022},
Volume = {64},
Number = {3},
Pages = {553-563},
Month = {MAR},
Abstract = {Purpose Patients with Moyamoya Angiopathy (MMA) require hemodynamic
   evaluation to assess the risk of stroke. Assessment of cerebral blood
   flow with {[}O-15]water PET and acetazolamide challenge is the
   diagnostic standard for the evaluation of the cerebral perfusion reserve
   (CPR). Estimation of the cerebrovascular reactivity (CVR) by use of
   breath-hold-triggered fMRI (bh-fMRI) as an index of CPR has been
   proposed as a reliable and more readily available approach. Recent
   findings suggest the use of resting-state fMRI (rs-fMRI) which requires
   minimum patient compliance. The aim of this study was to compare rs-fMRI
   to bh-fMRI and {[}O-15]water PET in patients with MMA. Methods Patients
   with MMA underwent rs-fMRI and bh-fMRI in the same MRI session. Maps of
   the CVR gained by both modalities were compared retrospectively by
   calculating the correlation between the mean CVR of 12 volumes of
   interest. Additionally, the rs-maps of a subgroup of patients were
   compared to CPR-maps gained by {[}O-15]water PET. Results The comparison
   of the rs-maps and the bh-maps of 24 patients revealed a good
   correlation (Pearson's r = 0.71 +/- 0.13; preoperative patients:
   Pearson's r = 0.71 +/- 0.17; postoperative patients: Pearson's r = 0.71
   +/- 0.11). The comparison of 7 rs-fMRI data sets to the corresponding
   {[}O-15]water PET data sets also revealed a high level of agreement
   (Pearson's r = 0.80 +/- 0.19). Conclusion The present analysis indicates
   that rs-fMRI might be a promising non-invasive method with almost no
   patient cooperation needed to evaluate the CVR. Further prospective
   studies are required.},
Publisher = {SPRINGER},
Address = {ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES},
