@article{ WOS:000438864900001,
Author = {Yang, Biao and Cao, Jinmeng and Zhou, Tiantong and Dong, Li and Zou,
   Ling and Xiang, Jianbo},
Title = {Exploration of Neural Activity under Cognitive Reappraisal Using
   Simultaneous EEG-fMRI Data and Kernel Canonical Correlation Analysis},
Journal = {COMPUTATIONAL AND MATHEMATICAL METHODS IN MEDICINE},
Year = {2018},
Volume = {2018},
Abstract = {Background. Neural activity under cognitive reappraisal can be more
   accurately investigated using simultaneous EEG-(electroencephalography)
   fMRI (functional magnetic resonance imaging) than using EEG or fMRI
   only. Complementary spatiotemporal information can be found from
   simultaneous EEG-fMRI data to study brain function. Method. An effective
   EEG-fMRI fusion framework is proposed in this work. EEG-fMRI data is
   simultaneously sampled on fifteen visually stimulated healthy adult
   participants. Net-station toolbox and empirical mode decomposition are
   employed for EEG denoising. Sparse spectral clustering is used to
   construct fMRI masks that are used to constrain fMRI activated regions.
   A kernel-based canonical correlation analysis is utilized to fuse
   nonlinear EEG-fMRI data. Results. The experimental results show a
   distinct late positive potential (LPP, latency 200-700 ms) from the
   correlated EEG components that are reconstructed from nonlinear EEG-fMRI
   data. Peak value of LPP under reappraisal state is smaller than that
   under negative state, however, larger than that under neutral state. For
   correlated fMRI components, obvious activation can be observed in
   cerebral regions, e.g., the amygdala, temporal lobe, cingulate gyrus,
   hippocampus, and frontal lobe. Meanwhile, in these regions, activated
   intensity under reappraisal state is obviously smaller than that under
   negative state and larger than that under neutral state. Conclusions.
   The proposed EEG-fMRI fusion approach provides an effective way to study
   the neural activities of cognitive reappraisal with high spatiotemporal
   resolution. It is also suitable for other neuroimaging technologies
   using simultaneous EEG-fMRI data.},
