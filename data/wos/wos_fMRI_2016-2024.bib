@article{ WOS:000382486500010,
Author = {Rath, Jakob and Wurnig, Moritz and Fischmeister, Florian and Klinger,
   Nicolaus and Hoellinger, Ilse and Geissler, Alexander and Aichhorn,
   Markus and Foki, Thomas and Kronbichler, Martin and Nickel, Janpeter and
   Siedentopf, Christian and Staffen, Wolfgang and Verius, Michael and
   Golaszewski, Stefan and Koppelstaetter, Florian and Auff, Eduard and
   Felber, Stephan and Seitz, Ruediger J. and Beisteiner, Roland},
Title = {Between- and Within-Site Variability of fMRI Localizations},
Journal = {HUMAN BRAIN MAPPING},
Year = {2016},
Volume = {37},
Number = {6},
Pages = {2151-2160},
Month = {JUN},
Abstract = {This study provides first data about the spatial variability of fMRI
   sensorimotor localizations when investigating the same subjects at
   different fMRI sites. Results are comparable to a previous patient
   study. We found a median between-site variability of about 6 mm
   independent of task (motor or sensory) and experimental standardization
   (high or low). An intraclass correlation coefficient analysis using data
   quality measures indicated a major influence of the fMRI site on
   variability. In accordance with this, within-site localization
   variability was considerably lower (about 3 mm). We conclude that the
   fMRI site is a considerable confound for localization of brain activity.
