@article{ WOS:000480554200001,
Author = {Mele, Giulia and Cavaliere, Carlo and Alfano, Vincenzo and Orsini, Mario
   and Salvatore, Marco and Aiello, Marco},
Title = {Simultaneous EEG-fMRI for Functional Neurological Assessment},
Journal = {FRONTIERS IN NEUROLOGY},
Year = {2019},
Volume = {10},
Month = {AUG 13},
Abstract = {The increasing incidence of neurodegenerative and psychiatric diseases
   requires increasingly sophisticated tools for their diagnosis and
   monitoring. Clinical assessment takes advantage of objective parameters
   extracted by electroencephalogram and magnetic resonance imaging (MRI)
   among others, to support clinical management of neurological diseases.
   The complementarity of these two tools can be now emphasized by the
   possibility of integrating the two technologies in a hybrid solution,
   allowing simultaneous acquisition of the two signals by the novel
   EEG-fMRI technology. This review will focus on simultaneous EEG-fMRI
   technology and related early studies, dealing about issues related to
   the acquisition and processing of simultaneous signals, and including
   critical discussion about clinical and technological perspectives.},
